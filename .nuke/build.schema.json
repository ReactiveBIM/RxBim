{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Build Schema",
  "$ref": "#/definitions/build",
  "definitions": {
    "build": {
      "type": "object",
      "properties": {
        "Algorithm": {
          "type": "string",
          "description": "Digest algorithm"
        },
        "Cert": {
          "type": "string",
          "description": "Certificate path"
        },
        "Configuration": {
          "type": "string",
          "description": "Configuration to build - Default is 'Debug' (local) or 'Release' (server)",
          "enum": [
            "Debug",
            "Release"
          ]
        },
        "Continue": {
          "type": "boolean",
          "description": "Indicates to continue a previously failed build attempt"
        },
        "Csp": {
          "type": "string",
          "description": "CSP containing for Private key"
        },
        "Help": {
          "type": "boolean",
          "description": "Shows the help text for this build assembly"
        },
        "Host": {
          "type": "string",
          "description": "Host for execution. Default is 'automatic'",
          "enum": [
            "AppVeyor",
            "AzurePipelines",
            "Bamboo",
            "Bitrise",
            "GitHubActions",
            "GitLab",
            "Jenkins",
            "Rider",
            "SpaceAutomation",
            "TeamCity",
            "Terminal",
            "TravisCI",
            "VisualStudio",
            "VSCode"
          ]
        },
        "NoLogo": {
          "type": "boolean",
          "description": "Disables displaying the NUKE logo"
        },
        "NugetSource": {
          "type": "string",
          "description": "NuGet source URL"
        },
        "NugetSymbolSource": {
          "type": "string",
          "description": "NuGet symbol source URL"
        },
        "Partition": {
          "type": "string",
          "description": "Partition to use on CI"
        },
        "Plan": {
          "type": "boolean",
          "description": "Shows the execution plan (HTML)"
        },
        "PrivateKey": {
          "type": "string",
          "description": "Private key container"
        },
        "Profile": {
          "type": "array",
          "description": "Defines the profiles to load",
          "items": {
            "type": "string"
          }
        },
        "Project": {
          "type": "string",
          "description": "Select project"
        },
        "ProjectForPublish": {
          "type": "string",
          "description": "Project name"
        },
        "RevitVersion": {
          "type": "string"
        },
        "Root": {
          "type": "string",
          "description": "Root directory during build execution"
        },
        "ServerUrl": {
          "type": "string",
          "description": "Timestamp server URL"
        },
        "Skip": {
          "type": "array",
          "description": "List of targets to be skipped. Empty list skips all dependencies",
          "items": {
            "type": "string",
            "enum": [
              "BuildFromTag",
              "BuildMsi",
              "BuildMsiForProduction",
              "BuildMsiForTesting",
              "CheckCurrentBranch",
              "CheckProductionVersion",
              "CheckStageVersion",
              "CheckUncommitted",
              "Clean",
              "CleanOutput",
              "Compile",
              "CompileToTemp",
              "CopyDebugAddin",
              "CopyOutput",
              "GenerateAdditionalFiles",
              "GeneratePackageContentsFile",
              "GenerateProjectProps",
              "InstallWixTools",
              "Pack",
              "Publish",
              "Push",
              "Restore",
              "SetProjects",
              "SignAssemblies",
              "Tag",
              "TagProject",
              "Test"
            ]
          }
        },
        "Solution": {
          "type": "string",
          "description": "Path to a solution file that is automatically loaded"
        },
        "Target": {
          "type": "array",
          "description": "List of targets to be invoked. Default is '{default_target}'",
          "items": {
            "type": "string",
            "enum": [
              "BuildFromTag",
              "BuildMsi",
              "BuildMsiForProduction",
              "BuildMsiForTesting",
              "CheckCurrentBranch",
              "CheckProductionVersion",
              "CheckStageVersion",
              "CheckUncommitted",
              "Clean",
              "CleanOutput",
              "Compile",
              "CompileToTemp",
              "CopyDebugAddin",
              "CopyOutput",
              "GenerateAdditionalFiles",
              "GeneratePackageContentsFile",
              "GenerateProjectProps",
              "InstallWixTools",
              "Pack",
              "Publish",
              "Push",
              "Restore",
              "SetProjects",
              "SignAssemblies",
              "Tag",
              "TagProject",
              "Test"
            ]
          }
        },
        "Verbosity": {
          "type": "string",
          "description": "Logging verbosity during build execution. Default is 'Normal'",
          "enum": [
            "Minimal",
            "Normal",
            "Quiet",
            "Verbose"
          ]
        }
      }
    }
  }
}