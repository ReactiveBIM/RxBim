<Project>
    <PropertyGroup Condition="'$(NotRefenceApi)' == 'true'">
        <PackageId>$(MSBuildProjectName)</PackageId>
    </PropertyGroup>

    <PropertyGroup Condition="'$(NotRefenceApi)' != 'true'">
        <PackageId>$(MSBuildProjectName).$(ApplicationVersion)</PackageId>
    </PropertyGroup>
    
    <PropertyGroup>
        <AssemblyName>$(PackageId).$(Version)</AssemblyName>
    </PropertyGroup>
</Project>